{
  "name": "mfg-drone-frontend",
  "version": "1.0.0",
  "description": "MFG���������q���� ���Ȩ��",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "NODE_ENV=production tsc && vite build --mode production",
    "build:analyze": "npm run build:prod && npx vite-bundle-analyzer",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test --project=regression",
    "test:e2e:headed": "playwright test --project=regression --headed",
    "test:e2e:debug": "playwright test --project=regression --debug",
    "test:e2e:smoke": "playwright test --project=smoke",
    "test:e2e:critical": "playwright test --project=critical",
    "test:e2e:performance": "playwright test --project=performance",
    "test:e2e:security": "playwright test --project=security",
    "test:e2e:accessibility": "playwright test --project=accessibility",
    "test:e2e:tracking": "playwright test --project=tracking",
    "test:e2e:mobile": "playwright test --project=mobile-chrome",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:full": "playwright test --project=full",
    "test:e2e:parallel": "playwright test --workers=4",
    "test:e2e:serial": "playwright test --workers=1",
    "test:e2e:report": "playwright show-report",
    "test:e2e:trace": "playwright show-trace",
    "test:ci": "vitest run --coverage && playwright test --project=smoke",
    "test:ci:full": "vitest run --coverage && playwright test --project=regression",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "lint:a11y": "eslint src --ext ts,tsx --rule 'jsx-a11y/*: error'",
    "lint:security": "eslint src --ext ts,tsx --rule 'no-console: error' --rule 'no-debugger: error' --rule 'no-eval: error'",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "preview:prod": "vite preview --mode production",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.test.yml up --abort-on-container-exit",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up",
    "docker:build": "docker build -t mfg-drone-frontend:latest .",
    "security:audit": "npm audit --audit-level=moderate",
    "clean": "rm -rf dist node_modules/.vite test-results",
    "clean:install": "npm run clean && npm ci"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "@reduxjs/toolkit": "^1.9.3",
    "react-redux": "^8.0.5",
    "@mui/material": "^5.11.10",
    "@mui/icons-material": "^5.11.9",
    "@mui/lab": "^5.0.0-alpha.119",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "axios": "^1.3.4",
    "socket.io-client": "^4.6.1",
    "chart.js": "^4.2.1",
    "react-chartjs-2": "^5.2.0",
    "react-hook-form": "^7.43.5",
    "@hookform/resolvers": "^2.9.11",
    "yup": "^1.0.2",
    "zod": "^3.20.6",
    "notistack": "^3.0.1",
    "date-fns": "^2.29.3",
    "@tanstack/react-query": "^4.24.6",
    "react-webcam": "^7.0.1",
    "react-dropzone": "^14.2.3",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/lodash-es": "^4.17.7",
    "@vitejs/plugin-react": "^3.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vitest": "^0.29.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "jest-axe": "^8.0.0",
    "eslint": "^8.35.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "prettier": "^2.8.4",
    "msw": "^1.1.0",
    "@playwright/test": "^1.31.0",
    "@vitest/coverage-v8": "^0.29.1",
    "@vitest/ui": "^0.29.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "terser": "^5.16.6",
    "vite-bundle-analyzer": "^0.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}